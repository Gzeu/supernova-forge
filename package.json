{
  "name": "supernova-forge",
  "version": "1.0.0",
  "description": "ðŸš€ Advanced MultiversX dApp Development Platform with Supernova Updates, Next.js 15, Enterprise AI Integration, and Professional DeFi/NFT/Gaming Tools - 2025 Edition",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "playwright test",
    "test:contracts": "cd contracts && cargo test",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "contracts:build": "cd contracts && cargo build --release --target=wasm32-unknown-unknown",
    "contracts:test": "cd contracts && cargo test",
    "contracts:deploy:devnet": "cd contracts && npm run deploy:devnet",
    "contracts:deploy:testnet": "cd contracts && npm run deploy:testnet",
    "contracts:deploy:mainnet": "cd contracts && npm run deploy:mainnet",
    "analyze": "@next/bundle-analyzer",
    "audit": "npm audit --audit-level moderate",
    "security": "npm run audit && node scripts/security-scan.js",
    "benchmark": "node scripts/benchmark-performance.js",
    "monitor": "node scripts/monitor-dapp.js",
    "health-check": "node scripts/health-check.js",
    "clean": "rimraf .next coverage dist",
    "prepare": "husky install",
    "release": "standard-version && git push origin main --tags",
    "deploy:staging": "vercel",
    "deploy:production": "vercel --prod",
    "docker:build": "docker build -t supernova-forge:latest .",
    "docker:run": "docker run -p 3000:3000 supernova-forge:latest"
  },
